{% if app.session.get("student_group") %}
    {% set storage =  app.session.get("student_group") %}
    {% if storage|length > 0 %}
        <div class="btn-group">
            <button type="button" class="btn btn-warning dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                {{ "student.storage" | trans }}<span class="caret"></span><span class="badge">{{ storage|length }}</span>
            </button>
            <ul class="dropdown-menu">
                {% for student in students %}
                    <li>{{ student.name }}</li>         
                    {% endfor %}
            </ul>
        </div>
    {%endif%}
{% else %}
    <div><button class="btn btn-warning" type="button">{{ "student.empty_storage" | trans }}</button></div>
    {%endif %}        
    {#{% block javascripts %}
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
{% endblock %}#}